    "genre_list = []                                    #Creating a list to store genres\n",
    "for genre in movies['genres']:                     #Iterate through the genres column\n",
    "    genre_split = genre.split('|')                 #Split values from genres column using '|' delimiter\n",
    "    for item in genre_split:                       #Appending each of the splitted genres in the list created\n",
    "        if item not in genre_list: genre_list.append(item)\n",
    "\n",
    "for genre in genre_list:                            \n",
    "    movies[genre] = 0                              #Setting initial values to 0\n",
    "\n",
    "for index,row in movies.iterrows():                #Iteration through the rows in movies data\n",
    "    for current_genre in genre_list:               #Comparing for every splitted genre in genre_list and the values from genres column\n",
    "        if current_genre in row['genres']:         \n",
    "            movies.loc[index,current_genre] = 1    #If the genre is present in both then assign 1 as value\n",
    "            \n",
    "movies_sub = movies.iloc[:,4:]\n",
    "movies_sub.head()"
   ]
