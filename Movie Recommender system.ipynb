   "source": [
    "def get_similar_movies(movie_id, sim): \n",
    "    \n",
    "    title = movies_list.loc[movies_list.movieId==movie_id,'title'] #For the given movied return the title of that movie\n",
    "    title = np.array(title)[0]                                     #To take the specific values \n",
    "    \n",
    "    similarity = sig.loc[title,:]                                  #Similar movies with scores for title and movieid\n",
    "    \n",
    "    scores = np.array(similarity)                                  #Taking out the scores for similar movies obtained\n",
    "    \n",
    "    related = sig.loc[title,:][sig.loc[title,:]>sim].index         #Return movies with only the scores above the passed parameter score\n",
    "    \n",
    "    return(list(related))"
   ]
