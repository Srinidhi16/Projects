   "source": [
    "### Find out the genre that occurred maximum number of times"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 235,
   "id": "c3e93a4f",
   "metadata": {},
   "outputs": [],
   "source": [
    "def count_of_genres(genres):\n",
    "    \"\"\"Create a list by splitting the genres from pop_genre dataframe and retun the count of each genre\"\"\"\n",
    "    \n",
    "    genre_list = []\n",
    "    for g in genres:\n",
    "        split_genre = g.split('|')\n",
    "        genre_list.extend(split_genre)\n",
    "    return Counter(genre_list)\n",
    "\n",
    "#create a counter dictionary to get the count of each genres\n",
    "genre_dict = count_of_genres(pop_genre['genres'])\n",
    "\n",
    "# create dataframe for frequency graph\n",
    "genre_dict = nltk.FreqDist(genre_dict)\n",
    "\n",
    "genre = pd.DataFrame({'genres': list(genre_dict.keys()), 'count': list(genre_dict.values())})"
