   "source": [
    "#Similar users to get\n",
    "n = 10\n",
    "\n",
    "#set threshold to find most similar users\n",
    "threshold = 0.8\n",
    "\n",
    "#Get n similar users\n",
    "similar_users = matrix[matrix[user] > threshold][user].sort_values(ascending=False)[:n]\n",
    "\n",
    "print(f'The n similar users for userid {user} are below', similar_users) "
   ]
